#include "msp430g2553.h"
#include "/../header/ggiz_header.h"
;----------------------------------------------------------------------------------------------
        NAME   CONFIG_UART_16MHZ
        PUBLIC CONFIG_UART_9600
        RSEG   CODE           
;----------------------------------------------------------------------------------------------
CONFIG_UART_9600 ; 
;----------------------------------------------------------------------------------------------
              MOV.B    #00001h,&UCA0CTL1
              MOV.B    #00000h,&UCA0CTL0
              MOV.B    #00081h,&UCA0CTL1
              MOV.B    #00082h,&UCA0BR0   
              MOV.B    #00006h,&UCA0BR1
              MOV.B    #0000Ch,&UCA0MCTL
              MOV.B    #00080h,&UCA0CTL1
__INI_TRANS:  BIS.B    #UCA0RXIE,&IE2
              BIS.B    #UCA0TXIE,&IE2
              BIC.B    #UCA0RXIFG,&IFG2
              BIC.B    #UCA0TXIFG,&IFG2   
              RET
;----------------------------------------------------------------------------------------------
;----------------------------------------------------------------------------------------------
        END  
;----------------------------------------------------------------------------------------------